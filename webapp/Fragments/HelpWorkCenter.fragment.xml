<c:FragmentDefinition
    id="HelpWorkCenter"
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
>
    <SelectDialog
        id="idWorkCenterDialog"
        noDataText="No Work Center Found"
        growing="true"
        title="Select Work Center"
        search="_onWorkCenterSearch"
        confirm="_WorkCenterSelect"
        cancel="_onDialogClose"
        items="{WorkCenterModel>/WorkCenterData}"
    >
        <StandardListItem
            title="{WorkCenterModel>Workcenter}"
            iconDensityAware="false"
            iconInset="false"
            type="Active"
        />
    </SelectDialog>
</c:FragmentDefinition>
