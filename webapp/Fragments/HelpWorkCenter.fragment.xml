<c:FragmentDefinition
    id="HelpWorkCenter"
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
>
    <SelectDialog
        id="idWorkCenterDialog"
        noDataText="{i18n>NoData}"
        growing="true"
        title="{i18n>WorkCenter}"
        search="_onWorkCenterSearch"
        confirm="_WorkCenterSelect"
        cancel="_WorkCenterSelect"
        items="{WorkCenterModel>/WorkCenterData}"
    >
        <StandardListItem
            title="{WorkCenterModel>Data01}"
            description="{WorkCenterModel>Data02}"
            iconDensityAware="false"
            iconInset="false"
            type="Active"
        />
    </SelectDialog>
</c:FragmentDefinition>
