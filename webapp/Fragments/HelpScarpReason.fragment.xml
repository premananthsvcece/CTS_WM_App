<c:FragmentDefinition
    id="HelpScarpReason"
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
>
    <SelectDialog
        id="idScarpReasonDialog"
        noDataText="{i18n>NoData}"
        title="{i18n>SelScrapReason}"
        search="_onScarpReasonSearch"
        confirm="_ScarpReasonSelect"
        cancel="_ScarpReasonSelect"
        items="{ScarpReasonModel>/ScarpReasonData}"
    >
        <StandardListItem
            title="{ScarpReasonModel>Data03}"
            description="{ScarpReasonModel>Data04}"
            iconDensityAware="false"
            iconInset="false"
            type="Active"
        />
    </SelectDialog>
</c:FragmentDefinition>
