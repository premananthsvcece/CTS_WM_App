<c:FragmentDefinition
    id="idPostAction"
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core"
    xmlns:uiTable="sap.ui.table"
    autoFocus="false"
>
    <Dialog
        id="idDialogPost"
        title="{i18n>PostAction}"
        contentWidth="75%"
    >
        <l:VerticalLayout
            id="idVerticalLayoutPost"
            class="sapUiContentPadding"
            width="auto"
        >
            <l:content>
                <IconTabBar
                    id="idIconTabBarMulti"
                    expanded="{device>/isNoPhone}"
                    class="sapUiResponsiveContentPadding"
                >
                    <items>
                        <IconTabFilter
                            key="Postinfo"
                            text="{i18n>PostInfo}"
                        >
                            <List
                                id="idPostList"
                                class="sapUiContentPadding"
                                headerText="{i18n>StartAction}"
                                showSeparators="None"
                                backgroundDesign="Transparent"
                            >
                                <InputListItem
                                    label="{i18n>OprDetails}"
                                    visible="false"
                                >
                                    <Input
                                        id="idPostOperator"
                                        placeholder="Operator No"
                                        type="Text"
                                        enabled="false"
                                        width="15rem"
                                    />
                                </InputListItem>
                                <InputListItem
                                    label="{i18n>Plant}"
                                    visible="false"
                                >
                                    <Input
                                        id="idSelectPostPlant"
                                        type="Text"
                                        enabled="false"
                                        width="15rem"
                                    />
                                </InputListItem>
                                <InputListItem label="{i18n>TotalTime}">
                                    <Input
                                        id="idPostTotalTime"
                                        type="Number"
                                        change="onPostTotalChange"
                                        enabled="false"
                                        width="15rem"
                                        value="{
                                            type: 'sap.ui.model.odata.type.Decimal'
                                        }"
                                    />
                                </InputListItem>
                                <InputListItem
                                    label="{i18n>SetupTime}"
                                    visible="false"
                                >
                                    <Input
                                        id="idPostSetupTIme"
                                        type="Number"
                                        change="onPostSetupChange"
                                        width="15rem"
                                        visible="false"
                                        value="{
                                            type: 'sap.ui.model.odata.type.Decimal'
                                        }"
                                    />
                                </InputListItem>
                                <!-- type="Number" -->
                                <InputListItem label="{i18n>PostQuan} *">
                                    <Input
                                        id="idPostQuantity"
                                        required="true"
                                        change="onPostQuantityChange"
                                        valueLiveUpdate="true"
                                        width="15rem"
                                        value="{
                                            type: 'sap.ui.model.odata.type.Decimal'
                                        }"
                                    />
                                </InputListItem>
                                <InputListItem
                                    id="idPostBinDet01"
                                    label="{i18n>BinDet} *"
                                    visible="false"
                                >
                                    <Input
                                        id="idPostBinDet"
                                        required="true"
                                        change="onPostBinDetChange"
                                        showValueHelp="true"
                                        valueHelpRequest="onPostBinDetHelpRequest"
                                        width="15rem"
                                        visible="false"
                                    />
                                </InputListItem>
                                <InputListItem label="{i18n>ConfirmType} *">
                                    <Select
                                        id="idPostConfType"
                                        forceSelection="true"
                                        selectedKey="P"
                                        width="15rem"
                                    >
                                        <core:Item
                                            key="P"
                                            text="{i18n>PartConfirm}"
                                        />
                                        <core:Item
                                            key="F"
                                            text="{i18n>FinalConfirm}"
                                        />
                                    </Select>
                                </InputListItem>
                            </List>
                            <uiTable:Table
                                id="idPostScarpList"
                                visibleRowCount="3"
                                visibleRowCountMode="Interactive"
                                class="sapUiContentPadding"
                                rows="{PostScarpModel>/PostScarpData}"
                                alternateRowColors="true"
                                enableSelectAll="false"
                                selectionBehavior="RowOnly"
                                selectionMode="Single"
                                enableCellFilter="true"
                            >
                                <uiTable:extension>
                                    <OverflowToolbar>
                                        <Title level="H2" />
                                        <ToolbarSpacer />
                                        <Button
                                            text="Add"
                                            type="Transparent"
                                            press="onScarpPostAdd"
                                            icon="sap-icon://add"
                                            visible="false"
                                        />
                                        <Button
                                            text="Copy"
                                            type="Transparent"
                                            press="onScarpPostCopy"
                                            icon="sap-icon://copy"
                                            visible="false"
                                        />
                                        <Button
                                            text="Delete"
                                            type="Attention"
                                            press="onScarpPostDel"
                                            icon="sap-icon://delete"
                                            visible="false"
                                        />
                                    </OverflowToolbar>
                                </uiTable:extension>
                                <uiTable:noData text="{i18n>NoData}" />
                                <uiTable:columns>
                                    <uiTable:Column width="20%">
                                        <Label text="{i18n>ScarpType} *" />
                                        <uiTable:template>
                                            <Input
                                                showValueHelp="true"
                                                showSuggestion="true"
                                                valueHelpRequest="onScarpPostReasonRequest"
                                                value="{PostScarpModel>Data06}"
                                                placeholder="Scarp Reason"
                                                change="onidScarpPostReasonChange"
                                                type="Text"
                                                valueLiveUpdate="false"
                                                required="true"
                                                enabled="false"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column width="50%">
                                        <Label text="{i18n>Description}" />
                                        <uiTable:template>
                                            <Text text="{PostScarpModel>Data07}" />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column width="20%">
                                        <Label text="{i18n>ScarpQuan}" />
                                        <uiTable:template>
                                            <Input
                                                type="Number"
                                                required="true"
                                                value="{PostScarpModel>Data05}"
                                                textAlign="Right"
                                                enabled="false"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                </uiTable:columns>
                            </uiTable:Table>
                            <uiTable:Table
                                id="idPostComponentList101"
                                visibleRowCount="3"
                                visibleRowCountMode="Fixed"
                                class="sapUiContentPadding"
                                rows="{ComponentModel101>/ComponentData101}"
                                alternateRowColors="true"
                                enableSelectAll="false"
                                selectionBehavior="RowOnly"
                                selectionMode="Single"
                                enableCellFilter="true"
                            >
                                <uiTable:extension>
                                    <OverflowToolbar>
                                        <Title level="H2" />
                                        <ToolbarSpacer />
                                        <Button
                                            id="id101Add"
                                            text="Add"
                                            type="Transparent"
                                            press="onPostCompAdd101"
                                            icon="sap-icon://add"
                                            visible="false"
                                        />
                                        <Button
                                            id="id101Copy"
                                            text="Copy"
                                            type="Transparent"
                                            press="onPostCompCopy101"
                                            icon="sap-icon://copy"
                                        />
                                        <Button
                                            id="id101Del"
                                            text="Delete"
                                            type="Attention"
                                            press="onPostCompDel101"
                                            icon="sap-icon://delete"
                                        />
                                    </OverflowToolbar>
                                </uiTable:extension>
                                <uiTable:noData text="{i18n>NoData}" />
                                <uiTable:columns>
                                    <uiTable:Column width="12%">
                                        <Label text="{i18n>ItemNo}" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel101>Data01}"
                                                enabled="false"
                                                valueHelpRequest="onMaterailHelpRequest"
                                                showValueHelp="true"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column width="10%">
                                        <Label text="{i18n>ComponentDescr}" />
                                        <uiTable:template>
                                            <Text text="{ComponentModel101>Data02}" />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column
                                        width="7%"
                                        visible="false"
                                    >
                                        <Label
                                            text="{i18n>Plant}"
                                            visible="false"
                                        />
                                        <uiTable:template>
                                            <Text
                                                text="{ComponentModel101>Data03}"
                                                visible="false"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column
                                        width="7%"
                                        visible="false"
                                    >
                                        <Label
                                            text="{i18n>Location}"
                                            visible="false"
                                        />
                                        <uiTable:template>
                                            <Text
                                                text="{ComponentModel101>Data04}"
                                                visible="false"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column width="7%">
                                        <Label text="{i18n>BinDet}" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel101>Data12}"
                                                showValueHelp="true"
                                                change="onPostBinDetChange"
                                                valueHelpRequest="onPostBinDetHelpRequest"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <!-- <uiTable:Column width="7%">
                                        <Label text="{i18n>PrdSupplyArea}" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel101>Data11}"
                                                valueHelpRequest="onSupplyAreaHelpRequest"
                                                showValueHelp="true"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column> -->
                                    <uiTable:Column
                                        width="7%"
                                        visible="false"
                                    >
                                        <Label text="{i18n>MovementType}" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel101>Data08}"
                                                valueHelpRequest="onMovementHelpRequest"
                                                showValueHelp="true"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column width="12%">
                                        <Label text="{i18n>BatchOnly} *" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel101>Data05}"
                                                valueHelpRequest="onBatchHelpRequest"
                                                showValueHelp="true"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column width="12%">
                                        <Label text="{i18n>RequriedQuan} *" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel101>Data06}"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column width="5%">
                                        <Label text="{i18n>UOM}" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel101>Data07}"
                                                enabled="{ComponentModel101>Data10}"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                </uiTable:columns>
                            </uiTable:Table>
                            <uiTable:Table
                                id="idPostComponentList261"
                                visibleRowCount="3"
                                visibleRowCountMode="Interactive"
                                class="sapUiContentPadding"
                                rows="{ComponentModel261>/ComponentData261}"
                                alternateRowColors="true"
                                enableSelectAll="false"
                                selectionBehavior="RowOnly"
                                selectionMode="Single"
                                enableCellFilter="true"
                            >
                                <uiTable:extension>
                                    <OverflowToolbar>
                                        <Title level="H2" />
                                        <ToolbarSpacer />
                                        <Button
                                            text="Add"
                                            type="Transparent"
                                            press="onPostCompAdd261"
                                            icon="sap-icon://add"
                                        />
                                        <Button
                                            text="Copy"
                                            type="Transparent"
                                            press="onPostCompCopy261"
                                            icon="sap-icon://copy"
                                        />
                                        <Button
                                            text="Delete"
                                            type="Attention"
                                            press="onPostCompDel261"
                                            icon="sap-icon://delete"
                                        />
                                    </OverflowToolbar>
                                </uiTable:extension>
                                <uiTable:noData text="{i18n>NoData}" />
                                <uiTable:columns>
                                    <uiTable:Column width="12%">
                                        <Label text="{i18n>Component}" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel261>Data01}"
                                                enabled="{ComponentModel261>Data10}"
                                                valueHelpRequest="onMaterailHelpRequest"
                                                showValueHelp="true"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column width="10%">
                                        <Label text="{i18n>ComponentDescr}" />
                                        <uiTable:template>
                                            <Text text="{ComponentModel261>Data02}" />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column
                                        width="7%"
                                        visible="false"
                                    >
                                        <Label
                                            text="{i18n>Plant}"
                                            visible="false"
                                        />
                                        <uiTable:template>
                                            <Text
                                                text="{ComponentModel261>Data03}"
                                                visible="false"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column
                                        width="7%"
                                        visible="false"
                                    >
                                        <Label
                                            text="{i18n>Location}"
                                            visible="false"
                                        />
                                        <uiTable:template>
                                            <Text
                                                text="{ComponentModel261>Data04}"
                                                visible="false"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column width="7%">
                                        <Label text="{i18n>PrdSupplyArea}" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel261>Data11}"
                                                valueHelpRequest="onSupplyAreaHelpRequest"
                                                showValueHelp="true"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column
                                        width="7%"
                                        visible="false"
                                    >
                                        <Label text="{i18n>MovementType}" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel261>Data08}"
                                                valueHelpRequest="onMovementHelpRequest"
                                                showValueHelp="true"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column width="12%">
                                        <Label text="{i18n>BatchOnly} *" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel261>Data05}"
                                                valueHelpRequest="onBatchHelpRequest"
                                                showValueHelp="true"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column width="12%">
                                        <Label text="{i18n>RequriedQuan} *" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel261>Data06}"
                                                enabled="{ComponentModel261>Data09}"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column width="5%">
                                        <Label text="{i18n>UOM}" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel261>Data07}"
                                                enabled="{ComponentModel261>Data10}"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                </uiTable:columns>
                            </uiTable:Table>
                            <uiTable:Table
                                id="idPostComponentList561"
                                visibleRowCount="3"
                                visibleRowCountMode="Fixed"
                                class="sapUiContentPadding"
                                rows="{ComponentModel561>/ComponentData561}"
                                alternateRowColors="true"
                                enableSelectAll="false"
                                selectionBehavior="RowOnly"
                                selectionMode="Single"
                                enableCellFilter="true"
                            >
                                <uiTable:extension>
                                    <OverflowToolbar>
                                        <Title level="H2" />
                                        <ToolbarSpacer />
                                        <Button
                                            text="Add"
                                            type="Transparent"
                                            press="onPostCompAdd561"
                                            icon="sap-icon://add"
                                        />
                                        <Button
                                            text="Copy"
                                            type="Transparent"
                                            press="onPostCompCopy561"
                                            icon="sap-icon://copy"
                                        />
                                        <Button
                                            text="Delete"
                                            type="Attention"
                                            press="onPostCompDel561"
                                            icon="sap-icon://delete"
                                        />
                                    </OverflowToolbar>
                                </uiTable:extension>
                                <uiTable:noData text="{i18n>NoData}" />
                                <uiTable:columns>
                                    <uiTable:Column width="12%">
                                        <Label text="{i18n>Component}" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel561>Data01}"
                                                enabled="{ComponentModel561>Data10}"
                                                valueHelpRequest="onMaterailHelpRequest"
                                                showValueHelp="true"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column width="10%">
                                        <Label text="{i18n>ComponentDescr}" />
                                        <uiTable:template>
                                            <Text text="{ComponentModel561>Data02}" />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column
                                        width="7%"
                                        visible="false"
                                    >
                                        <Label
                                            text="{i18n>Plant}"
                                            visible="false"
                                        />
                                        <uiTable:template>
                                            <Text
                                                text="{ComponentModel561>Data03}"
                                                visible="false"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column
                                        width="7%"
                                        visible="false"
                                    >
                                        <Label
                                            text="{i18n>Location}"
                                            visible="false"
                                        />
                                        <uiTable:template>
                                            <Text
                                                text="{ComponentModel561>Data04}"
                                                visible="false"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column width="7%">
                                        <Label text="{i18n>PrdSupplyArea}" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel561>Data11}"
                                                valueHelpRequest="onSupplyAreaHelpRequest"
                                                showValueHelp="true"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column
                                        width="7%"
                                        visible="false"
                                    >
                                        <Label text="{i18n>MovementType}" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel561>Data08}"
                                                valueHelpRequest="onMovementHelpRequest"
                                                showValueHelp="true"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column width="12%">
                                        <Label text="{i18n>BatchOnly} *" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel561>Data05}"
                                                valueHelpRequest="onBatchHelpRequest"
                                                showValueHelp="true"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column width="12%">
                                        <Label text="{i18n>RequriedQuan} *" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel561>Data06}"
                                                enabled="{ComponentModel>Data09}"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                    <uiTable:Column width="5%">
                                        <Label text="{i18n>UOM}" />
                                        <uiTable:template>
                                            <Input
                                                value="{ComponentModel561>Data07}"
                                                enabled="{ComponentModel561>Data10}"
                                            />
                                        </uiTable:template>
                                    </uiTable:Column>
                                </uiTable:columns>
                            </uiTable:Table>
                        </IconTabFilter>
                        <IconTabFilter
                            key="Activity"
                            text="{i18n>ActivityInfo}"
                        >
                            <Table
                                id="idPostActivityList"
                                class="sapUiContentPadding"
                                headerText="{i18n>ActivityDetails}"
                                items="{ActivityModel>/ActivityData}"
                                backgroundDesign="Transparent"
                            >
                                <columns>
                                    <Column
                                        demandPopin="true"
                                        width="15%"
                                    >
                                        <Text text="{i18n>ProdOrdNo}" />
                                    </Column>
                                    <Column
                                        demandPopin="true"
                                        width="10%"
                                    >
                                        <Text text="{i18n>OprNo}" />
                                    </Column>
                                    <Column demandPopin="true">
                                        <Text text="{i18n>OperStrtDate}" />
                                    </Column>
                                    <Column demandPopin="true">
                                        <Text text="{i18n>OperStrtTime}" />
                                    </Column>
                                    <Column demandPopin="true">
                                        <Text text="{i18n>OperEndDate}" />
                                    </Column>
                                    <Column demandPopin="true">
                                        <Text text="{i18n>OperEndTime}" />
                                    </Column>
                                    <Column demandPopin="true">
                                        <Text text="{i18n>TotalTime}" />
                                    </Column>
                                </columns>
                                <items>
                                    <ColumnListItem vAlign="Middle">
                                        <cells>
                                            <ObjectIdentifier title="{ActivityModel>Data01}" />
                                            <ObjectIdentifier title="{ActivityModel>Data02}" />
                                            <DatePicker
                                                value="{ActivityModel>Data03}"
                                                displayFormat="dd/MM/yy"
                                                enabled="false"
                                            />
                                            <TimePicker
                                                value="{ActivityModel>Data04}"
                                                displayFormat="hh:mm:ss"
                                                enabled="false"
                                            />
                                            <DatePicker
                                                value="{ActivityModel>Data05}"
                                                displayFormat="dd/MM/yy"
                                                enabled="false"
                                            />
                                            <TimePicker
                                                value="{ActivityModel>Data06}"
                                                displayFormat="hh:mm:ss"
                                                enabled="false"
                                            />
                                            <Input
                                                value="{ActivityModel>Data07}"
                                                enabled="false"
                                            />
                                        </cells>
                                    </ColumnListItem>
                                </items>
                            </Table>
                        </IconTabFilter>
                    </items>
                </IconTabBar>
                
             </l:content>

        </l:VerticalLayout>


        
          <Bar>
                <contentMiddle>
                    <Button
                        icon="sap-icon://accept"
                        class="sapUiSmallMarginBegin"
                        text="{i18n>Save}"
                        type="Success"
                        press="onConfirmPostPress"
                    />
                    <Button
                        icon="sap-icon://cancel"
                        class="sapUiSmallMarginBegin"
                        text="{i18n>Cancel}"
                        type="Negative"
                        press="onCancelPostPress"
                    />
                </contentMiddle>
            </Bar>

    </Dialog>
</c:FragmentDefinition>
