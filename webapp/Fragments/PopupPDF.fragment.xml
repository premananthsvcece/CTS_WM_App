<core:FragmentDefinition
    id="idDiagramBox"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <Dialog
        id="idDialogDiagram"
        state="Information"
        title="Material Diagram"
        contentWidth="75%"
        contentHeight="75%"
        titleAlignment="Center"
    >
        <PDFViewer
            id="idPDFViewer"
            isTrustedSource="true"
            errorPlaceholderMessage="{i18n>NoDisplay}"
        />
    </Dialog>
</core:FragmentDefinition>
